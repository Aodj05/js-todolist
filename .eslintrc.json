{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "no-alert": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "semi": false,
                "printWidth": 80,
                "tabWidth": 2,
                "endOfLine": "auto"
            }
        ]
    }
}
